{
    "version": "0.2.0",
    "configurations": [
    {
            "name": "Run with argument",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/tools",
            "env": {"CUDA_VISIBLE_DEVICES": "0"},
            "args": ["--cfg_file" ,
                    "cfgs/kitti_models/pv_rcnn_ssl_60.yaml",
                    "--split",
                    "train_0.01_1",
                    "--batch_size",
                    "1",
                    "--pretrained_model",
                    "/mnt/data/adat01/pv_rcnn/fully_sup/checkpoint_epoch_80.pth",
                    "--ckpt_save_interval",
                    "1",
                    "--repeat",
                    "1",
                    "--fix_random_seed",
                    "--extra_tag",
                    "consistency_debug", 
                    "--thresh",
                    "0.5,0.25,0.25", 
                    "--sem_thresh", 
                    "0.4,0.0,0.0", 
                    "--dbinfos",
                    "kitti_dbinfos_train_0.01_1_37.pkl",
                    "--set",
                    "MODEL.DYNAMIC_ULB_LOSS_WEIGHT.ENABLE",
                    "False",
                    "MODEL.ROI_HEAD.PROTO_INTER_LOSS.ENABLE",
                    "False"
            ],
    }
    ]
}