{
    "version": "0.2.0",
    "configurations": [
     {
         "name": "Run with argument",
         "type": "python",
         "request": "launch",
         "program": "${file}",
         "console": "integratedTerminal",
         "cwd": "${workspaceFolder}/tools",
         "env": {"CUDA_VISIBLE_DEVICES": "1"},
         "args": ["--cfg_file" ,
                 "cfgs/kitti_models/pv_rcnn_ssl_60.yaml",
                 "--batch_size",
                 "8",
                 "--pretrained_model",
                "../../ckpt/pv_rcnn/checkpoint_epoch_80.pth", // /mnt/data/adat01/ckpt/pv_rcnn/checkpoint_epoch_80.pth
                "--save_to_file",
                "--extra_tag",
                "rcnn_cls_sem_ori_lpcont",
                "--epochs",
                "40",
                 "--ckpt_save_interval",
                 "2",
                 "--repeat",
                 "5",
                 "--fix_random_seed",
                 "--thresh",
                 "0.5,0.25,0.25",
                 "--sem_thresh",
                 "0.4,0.4,0.4",
                 "--dbinfos",
                 "kitti_dbinfos_train_0.01_1_37.pkl",
                 "--split",
                 "train_0.01_1",
                 "--set",
                 "MODEL.STORE_SCORES_IN_PKL",
                 "False",
                 "MODEL.ROI_HEAD.ENABLE_PROTOTYPING",
                 "True",
                 "MODEL.ROI_HEAD.STG2_PROJ_FC_CFGS.ACTIVATION",
                 "ReLU",
                 "MODEL.ROI_HEAD.STG2_PROJ_FC_CFGS.BATCH_NORM",
                 "True",
                "MODEL.ROI_HEAD.STG2_PROJ_FC_CFGS.DP_RATIO",
                "0.1",
                "MODEL.ROI_HEAD.ENABLE_PROTO_CONTRASTIVE_LOSS",
                "False",
                "MODEL.ROI_HEAD.ENABLE_LPCONT_LOSS",
                "False",
                "MODEL.ROI_HEAD.ENABLE_ORI_LPCONT_LOSS",
                "False",
                "MODEL.ROI_HEAD.CLIP_CE",
                "False"
                ],
     }
    ]
 }
